#!/bin/sh
#PBS -j oe
#PBS -N eta-1.0
#PBS -l walltime=08:00:00
#PBS -l select=100:ncpus=1:mem=6gb

module load git
module load openmpi

. ~/miniconda3/etc/profile.d/conda.sh
conda activate ptti

YAML=eta-err-1.0.yaml
OUTPUT=eta-err-1.0

cd ~/ptti-data/eta-err
mpirun ptti --samples 100 -st --parallel -y $YAML -o $OUTPUT
